{
  "version": 2,
  "builds": [
    {
      "src": "nuxt/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "nuxt/.output/public"
      }
    },
    {
      "src": "vercel/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/vercel/index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}